# MongoDB Atlas connection string (from Railway MongoDB plugin or Atlas)
# Example: mongodb+srv://user:pass@cluster.mongodb.net/
MONGO_URL=mongodb+srv://YOUR_USER:YOUR_PASS@YOUR_CLUSTER.mongodb.net/

# Database name
DB_NAME=revops_garage

# Comma-separated allowed origins, or * for all
# Example: https://revops-frontend.up.railway.app
CORS_ORIGINS=https://YOUR-FRONTEND.up.railway.app

# Strong random secret for JWT signing
JWT_SECRET=change-this-to-a-long-random-secret
